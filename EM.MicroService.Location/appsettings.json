{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Location": "Server=localhost;Port=5432;Database=location;Username=user;Password=password"
  },
  "AllowedHosts": "*",
  "RabbitMqOptions": {
    "HostName": "localhost",
    "User": "user",
    "Password": "password",
    "PrefetchCount": "1",
    "Consumers": [
      {
        "Exchange": {
          "Name": "get_current_city",
          "Type": "direct"
        },
        "Queue": {
          "Name": "get_current_city_queue",
          "Durable": true,
          "Exclusive": false,
          "AutoDelete": true
        },
        "AutoAck": false
      },
      {
        "Exchange": {
          "Name": "get_all_cities",
          "Type": "direct"
        },
        "Queue": {
          "Name": "get_all_cities_queue",
          "Durable": true,
          "Exclusive": false,
          "AutoDelete": true
        },
        "AutoAck": false
      },
      {
        "Exchange": {
          "Name": "get_place_by_id",
          "Type": "direct"
        },
        "Queue": {
          "Name": "get_place_by_id_queue",
          "Durable": true,
          "Exclusive": false,
          "AutoDelete": true
        },
        "AutoAck": false
      },
      {
        "Exchange": {
          "Name": "update_place",
          "Type": "direct"
        },
        "Queue": {
          "Name": "update_place_queue",
          "Durable": true,
          "Exclusive": false,
          "AutoDelete": true
        },
        "AutoAck": false
      }
    ],
    "RpcCalls": [
      {
        "Exchange": {
          "Name": "get_fake_data",
          "Type": "direct"
        },
        "Consumer": {
          "Exchange": {
            "Name": "get_fake_data_reply",
            "Type": "direct"
          },
          "AutoAck": false
        }
      }
    ]
  }
}