{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings":{
    "Location": "Server=localhost;Port=5432;Database=location;Username=user;Password=password"
  },

  "ExternalIpServiceOptions":{
    "ExternalServices": [
      "https://api.ipify.org"
  ]},

  "AllowedHosts": "*",

  "RabbitMqOptions": {
    "HostName": "localhost",
    "User": "user",
    "Password": "password",
    "PrefetchCount": "1",
    "Consumers": [
      {
        "Exchange":{
          "Name": "get_location",
          "Type": "direct"
        },
        "Queue": {
          "Name": "get_location_queue",
          "Durable": true,
          "Exclusive": false,
          "AutoDelete": true
        },
        "AutoAck": false
      }
    ]
  }
}