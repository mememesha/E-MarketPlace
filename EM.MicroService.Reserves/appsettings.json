{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Reserves": "Server=localhost;Port=5432;Database=reserves;Username=user;Password=password"
  },
  "RabbitMqOptions": {
    "HostName": "localhost",
    "User": "user",
    "Password": "password",
    "PrefetchCount": "1",
    "Consumers": [
      {
        "Exchange": {
          "Name": "get_reserve_by_id",
          "Type": "direct"
        },
        "Queue": {
          "Name": "get_reserve_by_id_queue",
          "Durable": true,
          "Exclusive": false,
          "AutoDelete": true
        },
        "AutoAck": false
      },
      {
        "Exchange": {
          "Name": "add_reserve",
          "Type": "direct"
        },
        "Queue": {
          "Name": "add_reserve_queue",
          "Durable": true,
          "Exclusive": false,
          "AutoDelete": true
        },
        "AutoAck": false
      },
      {
        "Exchange": {
          "Name": "get_reserves_by_offer_id",
          "Type": "direct"
        },
        "Queue": {
          "Name": "get_reserves_by_offer_id_queue",
          "Durable": true,
          "Exclusive": false,
          "AutoDelete": true
        },
        "AutoAck": false
      }
    ]
  }
}