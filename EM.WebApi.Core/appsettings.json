{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  
  "AllowedHosts": "*",
  
  "ElasticsearchOptions" : {
    "Uri" : "http://localhost:9200"
  },
  
  "IS4" : {
    "Uri" : "https://localhost:10001",
    "Audience" : "webapi"
  },

  "RabbitMqOptions": {
    "HostName": "localhost",
    "User": "user",
    "Password": "password",
    "PrefetchCount": "1",
    "RpcCalls":[
      {
        "Exchange": {
          "Name": "get_location",
          "Type": "direct"
        },
        "Consumer":{
          "Exchange":{
            "Name": "get_location_reply",
            "Type": "direct"
          },
          "AutoAck": false
        }
      }
    ]
  }
          
}
