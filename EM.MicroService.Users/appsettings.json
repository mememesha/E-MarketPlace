{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Users": "Server=localhost;Port=5432;Database=users;Username=user;Password=password"
  },
  "RabbitMqOptions": {
    "HostName": "localhost",
    "User": "user",
    "Password": "password",
    "PrefetchCount": "1",
    "Consumers": [
      {
        "Exchange": {
          "Name": "get_user_by_id",
          "Type": "direct"
        },
        "Queue": {
          "Name": "get_user_by_id_queue",
          "Durable": true,
          "Exclusive": false,
          "AutoDelete": true
        },
        "AutoAck": false
      }
    ],
    "RpcCalls": [
      {
        "Exchange": {
          "Name": "get_fake_data",
          "Type": "direct"
        },
        "Consumer": {
          "Exchange": {
            "Name": "get_fake_data_reply",
            "Type": "direct"
          },
          "AutoAck": false
        }
      }
    ]
  }
}